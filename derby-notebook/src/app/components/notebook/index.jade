index:(element="j-notebook")

  j-header(
    notebook="{{notebook}}"
    currentCell="{{currentCell}}"
    cells="{{cells}}"
    user="{{user}}"
  )
  
  #site(style="display:block;")
    #ipython-main-app
      #notebook_panel
        #notebook
          #notebook-container.container
              each cells as #cell, #idx
                if #cell.cell_type is "markdown"
                  j-cell-markup(
                    cell="{{#cell}}"
                    currentCell="{{currentCell}}"
                    user="{{user}}"
                  )
                else
                  j-cell(
                    cell="{{#cell}}"
                    currentCell="{{currentCell}}"
                    user="{{user}}"
                  )
          .end_space
